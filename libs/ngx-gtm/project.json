{
  "name": "ngx-gtm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ngx-gtm/src",
  "prefix": "ngx-gtm",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ngx-gtm/ng-package.json",
        "tsConfig": "libs/ngx-gtm/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ngx-gtm/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ngx-gtm/jest.config.ts",
        "tsConfig": "libs/ngx-gtm/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "release": {
      "executor": "nx-release:build-update-publish",
      "options": {
        "libName": "ngx-gtm"
      }
    }
  }
}
